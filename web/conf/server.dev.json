{
  "logger": [
    {
      "type": "file",
      "filename": "logs/server.log",
      "maxLogSize": 10485760,
      "backups": 5
    },
    {
      "type": "console"
    }
  ],
  "poller": {
    "interval": 10000,
    "rootUrl": "http://localhost:8081",
    "retries": 7
  },
  "parser": {
    "fileLocation": "./data/structure.json",
    "mandatoryFields": ["name", "url"]
  }
}
